#include <stdio.h>

struct cust{ int id; char name[20]; char mob[12]; };

int main(){
    struct cust c[2];
    int id;
    for(int i=0;i<2;i++)
        scanf("%d %s %s",&c[i].id,c[i].name,c[i].mob);
    printf("Enter ID: "); scanf("%d",&id);
    for(int i=0;i<2;i++)
        if(c[i].id==id)
            printf("%s %s\n",c[i].name,c[i].mob);
    return 0;
}


Output:
1 Ravi 98765
2 Anil 91234
Enter ID: 2
Anil 91234
