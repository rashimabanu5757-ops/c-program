#include <stdio.h>

int main() {
    int n, i, j, temp;
    int marks[50];

    printf("Enter number of students: ");
    scanf("%d", &n);

    printf("Enter marks of students:\n");
    for (i = 0; i < n; i++) {
        scanf("%d", &marks[i]);
    }

    // Sorting marks (Bubble Sort)
    for (i = 0; i < n - 1; i++) {
        for (j = i + 1; j < n; j++) {
            if (marks[i] > marks[j]) {
                temp = marks[i];
                marks[i] = marks[j];
                marks[j] = temp;
            }
        }
    }

    printf("Sorted marks (Ascending order):\n");
    for (i = 0; i < n; i++) {
        printf("%d ", marks[i]);
    }

    return 0;
}
O/P:
Enter number of students: 5
Enter marks of students:
78 85 69 90 72
Sorted marks (Ascending order):
69 72 78 85 90
