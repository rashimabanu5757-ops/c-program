#include <stdio.h>

struct book{ int id; char title[20]; float price; };

int main(){
    struct book b[2];
    for(int i=0;i<2;i++)
        scanf("%d %s %f",&b[i].id,b[i].title,&b[i].price);
    for(int i=0;i<2;i++)
        printf("%d %s %.2f\n",b[i].id,b[i].title,b[i].price);
    return 0;
}


Output:

1 C 250
2 CPP 300
1 C 250.00
2 CPP 300.00
